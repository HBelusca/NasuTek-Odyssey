/* Default linker script, for normal executables */
OUTPUT_FORMAT("elf32-powerpc", "elf32-powerpc",
	      "elf32-powerpc")
OUTPUT_ARCH(powerpc:common)
SECTIONS
{
  .text :
  {
    __text_start__ = .;
    *(.text)
    *(init)
    *(INIT)
    __text_end__ = .;
  }
  .data :
  {
    *(.data)
    *(.data2)
    *(.rdata)
    *(.sdata)
    *(.pdata)
  }
  .rodata :
  {
    *(.rodata*)
    *(.got2)
    *(.eh_frame)
  }
  .idata :
  {
     *(.idata$2)
     SORT(*)(.idata$3)
     LONG(0); LONG(0); LONG(0); LONG(0); LONG(0);
     SORT(*)(.idata$4)
     SORT(*)(.idata$5)
     SORT(*)(.idata$6)
     SORT(*)(.idata$7)
  }
  .edata :
  {
    *(.edata)
  }
  .bss :
  {
    *(.sbss)
    *(.bss)
    *(COMMON)
  }  
  .rsrc :
  {
    *(.rsrc)
  }
  .rela :
  {
    *(.rela.*)
  }
}
