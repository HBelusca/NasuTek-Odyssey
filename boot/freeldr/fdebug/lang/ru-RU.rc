IDC_FDEBUG MENU DISCARDABLE
BEGIN
    POPUP "&Файл"
    BEGIN
        MENUITEM "&Соединение", IDM_FILE_CONNECT
        MENUITEM "&Отключение", IDM_FILE_DISCONNECT, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "О&чистить экран", IDM_FILE_CLEARDISPLAY
        MENUITEM SEPARATOR
        MENUITEM "&Начать захват", IDM_FILE_STARTCAPTURE
        MENUITEM "Ос&тановить захват", IDM_FILE_STOPCAPTURE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Локальный ответ", IDM_FILE_LOCALECHO
        MENUITEM SEPARATOR
        MENUITEM "&Выход", IDM_EXIT
    END
    POPUP "&Справка"
    BEGIN
        MENUITEM "&О программе...", IDM_ABOUT
    END
END


