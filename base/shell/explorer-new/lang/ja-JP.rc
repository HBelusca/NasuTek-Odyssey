FONT 9, "MS UI Gothic", 0, 0, 0x1
BEGIN
    GROUPBOX        "タスクバーの外観", IDC_STATIC, 6,6,240,121
    CONTROL         "", IDC_TASKBARPROP_TASKBARBITMAP, "Static", SS_BITMAP | SS_SUNKEN, 13,18,224,21
