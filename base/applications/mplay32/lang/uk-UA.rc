LANGUAGE LANG_UKRAINIAN, SUBLANG_DEFAULT

IDR_MAINMENU MENU
BEGIN
	POPUP "&Файл"
	BEGIN
		MENUITEM "&Відкрити...", IDM_OPEN_FILE
		MENUITEM "&Закрити", IDM_CLOSE_FILE
		MENUITEM SEPARATOR
		MENUITEM "&Вихід", IDM_EXIT
	END
	POPUP "&Довідка"
	BEGIN
		MENUITEM "&Про програму...", IDM_ABOUT
	END
END

