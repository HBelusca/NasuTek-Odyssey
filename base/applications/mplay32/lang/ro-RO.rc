LANGUAGE LANG_ROMANIAN, SUBLANG_DEFAULT

IDR_MAINMENU MENU
BEGIN
	POPUP "&Fișier"
	BEGIN
		MENUITEM "&Deschide...", IDM_OPEN_FILE
		MENUITEM "În&chide", IDM_CLOSE_FILE
