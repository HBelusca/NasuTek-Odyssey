	IDS_TOOLTIP_SEEKBACK "Sök bakåt"
	IDS_TOOLTIP_SEEKFORW "Sök framåt"
	IDS_TOOLTIP_FORWARD "Spola fremåt"
    IDS_APPTITLE "Odyssey Multimediaspelare"
