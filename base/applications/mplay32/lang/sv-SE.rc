LANGUAGE LANG_SWEDISH, SUBLANG_NEUTRAL

IDR_MAINMENU MENU
BEGIN
	POPUP "&Arkiv"
	BEGIN
		MENUITEM "&Öppna...", IDM_OPEN_FILE
		MENUITEM "&Stäng", IDM_CLOSE_FILE
		MENUITEM SEPARATOR
		MENUITEM "&Avsluta", IDM_EXIT
	END
	POPUP "&Hjälp"
	BEGIN
		MENUITEM "&Om...", IDM_ABOUT
	END
END

