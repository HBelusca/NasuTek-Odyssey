LANGUAGE LANG_ITALIAN, SUBLANG_NEUTRAL

IDR_MAINMENU MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Apri...",    IDM_OPEN_FILE
        MENUITEM "&Chiudi",     IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "&Esci",       IDM_EXIT
    END
    POPUP "&Aiuto"
    BEGIN
        MENUITEM "&Informazioni...", IDM_ABOUT
    END
END

