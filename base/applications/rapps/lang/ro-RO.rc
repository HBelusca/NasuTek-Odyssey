    LTEXT "", IDC_DOWNLOAD_STATUS, 10, 30, 200, 10, SS_CENTER
    PUSHBUTTON "A&nulează", IDCANCEL, 85, 58, 50, 15, WS_GROUP | WS_TABSTOP
END

