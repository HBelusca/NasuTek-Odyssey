	EDITTEXT IDC_DOWNLOAD_DIR_EDIT, 15, 86, 166, 12, WS_CHILD | WS_VISIBLE | WS_GROUP
	PUSHBUTTON "&Alege...", IDC_CHOOSE, 187, 85, 50, 14
