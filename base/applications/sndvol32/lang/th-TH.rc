LANGUAGE LANG_THAI, SUBLANG_DEFAULT

IDM_MAINMENU MENU DISCARDABLE
BEGIN
    POPUP "&ตัวเลือก"
    BEGIN
        MENUITEM "&คุณสมบัติ", IDC_PROPERTIES
        MENUITEM "ควบคุมแบบ&ขั้นสูง", IDC_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&ออกจากโปรแกรม", IDC_EXIT
    END
    POPUP "&ช่วยเหลือ"
    BEGIN
