    BEGIN
        MENUITEM "&Eigenschaften", IDC_PROPERTIES
        MENUITEM "Erweiterte Ein&stellungen", IDC_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&Beenden", IDC_EXIT
    END
    POPUP "&?"
    BEGIN
        MENUITEM "&Hilfethemen", IDC_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&Info", IDC_ABOUT
    END
END

