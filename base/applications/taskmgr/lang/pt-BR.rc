IDR_PROCESS_PAGE_CONTEXT MENU DISCARDABLE
BEGIN
    POPUP "DUMMY"
    BEGIN
        MENUITEM "&Finalizar processo",             ID_PROCESS_PAGE_ENDPROCESS
