            MENUITEM "Abai&xo do normal",           ID_PROCESS_PAGE_SETPRIORITY_BELOWNORMAL

            MENUITEM "&Baixa",                      ID_PROCESS_PAGE_SETPRIORITY_LOW

        END
        MENUITEM "Definir &afinidade...",         ID_PROCESS_PAGE_SETAFFINITY
