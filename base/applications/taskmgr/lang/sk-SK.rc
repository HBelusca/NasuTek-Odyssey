    MENUITEM "Ma&ximalizovať",                   ID_WINDOWS_MAXIMIZE
    MENUITEM "&Kaskáda",                    ID_WINDOWS_CASCADE
    MENUITEM "Presunúť d&opredu",             ID_WINDOWS_BRINGTOFRONT
END

