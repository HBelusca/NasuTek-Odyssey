	EDITTEXT        IDC_EVENTTEXTEDIT,14,81,247,108,ES_MULTILINE | ES_READONLY
	LTEXT           "説明(&D):",IDC_STATIC,15,70,39,8
	LTEXT           "日付:",IDC_STATIC,14,14,36,8
