CAPTION "Свойства события"
FONT 8, "MS Shell Dlg", 400, 0, 0x1
BEGIN
	DEFPUSHBUTTON   "&Закрыть",IDOK,14,258,55,14
	PUSHBUTTON      "&Предыдущее",IDPREVIOUS,78,258,55,14
	PUSHBUTTON      "С&ледующее",IDNEXT,142,258,55,14
