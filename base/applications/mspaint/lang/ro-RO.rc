    AUTORADIOBUTTON "cm", IDD_ATTRIBUTESRB2, 62, 69, 45, 10
    AUTORADIOBUTTON "Pixeli", IDD_ATTRIBUTESRB3, 112, 69, 45, 10
    GROUPBOX "Culori", IDD_ATTRIBUTESGROUP2, 6, 88, 158, 27
