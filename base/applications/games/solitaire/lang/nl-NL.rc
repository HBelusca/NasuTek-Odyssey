        MENUITEM "&Opties...",                 IDM_GAME_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&Afsluiten",                       IDM_GAME_EXIT
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Help-onder&werpen\tF1",               IDM_HELP_CONTENTS
        MENUITEM "&Over Solitaire",                      IDM_HELP_ABOUT
    END
END



