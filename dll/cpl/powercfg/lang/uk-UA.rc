    GROUPBOX        "Параметри",-1,5,35,225,55
    CONTROL         "За&вжди показувати значок на панелі завдань",IDC_SYSTRAYBATTERYMETER,"Button",BS_AUTOCHECKBOX |
                    WS_TABSTOP,15,45,200,10
